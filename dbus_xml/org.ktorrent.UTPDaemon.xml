<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
	<interface name="org.ktorrent.UTPDaemon">
	
	<signal name="acceptedPeer">
		<arg name="fifo" type="s" />
		<arg name="ip" type="s" />
		<arg name="port" type="i" />
	</signal>
	
	<method name="connectToPeer">
		<arg name="ip" type="s" direction="in" />
		<arg name="port" type="i" direction="in" />
		<arg name="fifo" type="s" direction="out" />
	</method>
	
	</interface>
</node>